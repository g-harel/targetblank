{
    "extends": [
        "tslint-config-airbnb"
    ],
    "rules": {
        "import-name": false,
        "indent": [true, "spaces", 4],
        "object-curly-spacing": [true, "never"],
        "quotemark": [true, "double"],
        "ter-arrow-parens": [true, "always"],
        "ter-indent": false
    }
}