{
  "scripts": {
    "build": "parcel build --no-source-maps -d .build app/index.html",
    "dev": " opn http://localhost:1324 && parcel -p 1324 -d .dist app/index.html",
    "lint": "eslint app/**/*.js"
  },
  "dependencies": {
    "okwolo": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.1",
    "node-sass": "^4.9.0",
    "opn-cli": "^3.1.0",
    "parcel-bundler": "^1.8.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ]
    }
  }
}
